{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "cache": false,
      "dependsOn": ["^generate"]
    },
    "migrate": {
      "cache": false
    },
    "seed": {
      "cache": false
    },
    "test:integration": {
      "cache": false
    },
    "test:system": {
      "cache": false
    },
    "test:unit": {},
    "verify": {}
  }
}
